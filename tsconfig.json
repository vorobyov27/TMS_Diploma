{
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "types":[
        "node",
        "superagent",
        "jest",
        "@wdio/globals/types",
        "expect-webdriverio",
        "@wdio/cucumber-framework",
        "@wdio/browser-runner"
      ],
      "esModuleInterop": true,
      "target": "ES6",
      "lib": ["DOM", "ES2016"],
      "forceConsistentCasingInFileNames": true, 
      "strict": true, 
      "skipLibCheck": true
    },
    "ts-node": {"esm": false}
}